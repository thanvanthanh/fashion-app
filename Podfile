source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '13.0'
inhibit_all_warnings!

target 'Base-ios' do
  use_frameworks!
  
  
 # Rx
  pod 'RxSwift'
  pod 'RxDataSources', '~> 5.0'
  pod 'NSObject+Rx' # https://github.com/RxSwiftCommunity/NSObject-Rx
  pod 'RxCocoa'
  pod 'RxGesture'
  pod 'RxSwiftExt'
  pod 'Action'

 # Networking
 pod 'Moya/RxSwift' # https://github.com/Moya/Moya

 # Animation
  pod 'lottie-ios'

 # UI
 pod 'SDWebImage'

 pod 'SwiftGen'
end
